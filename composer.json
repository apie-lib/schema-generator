{
  "name": "apie/schema-generator",
  "description": "Composer package of the apie library: schema generator",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\SchemaGenerator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Apie\\Tests\\SchemaGenerator\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.3",
    "apie/core": "self.version",
    "apie/dateformat-to-regex": "self.version",
    "devizzent/cebe-php-openapi": "^1.1.2"
  },
  "require-dev": {
    "apie/common-value-objects": "self.version",
    "apie/date-value-objects": "self.version",
    "apie/fixtures": "self.version",
    "apie/service-provider-generator": "0.11.3",
    "apie/text-value-objects": "self.version",
    "illuminate/support": "*",
    "phpunit/phpunit": "^11.5.2",
    "phpspec/prophecy-phpunit": "^2.2"
  },
  "extra": {
    "apie-objects": [
      "Apie\\SchemaGenerator\\Enums\\SchemaUsages"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}